#include <stdio.h>
#include <math.h>

int main()
{
    float N1,N2,N3,M;
    
    scanf("%f %f %f",&N1,&N2,&N3);
    M=N1+N2+N3/3;
    if(M>=6){
        printf("Aluno Aprovado\n");
        printf("Media do aluno: %.2f\n",M);
    }else{
        printf("Aluno Reprovado\n");
        printf("Media do aluno: %.2f\n",M);
    }

    return 0;
}