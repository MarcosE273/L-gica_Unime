#include <stdio.h>
#include <math.h>

int main()
{
    int I;
    
    scanf("%d",&I);
    if(I>=5 && I<=7){
        printf("Infaltil A\n");
    }else if(I>=8 && I<=10){
        printf("Infaltil B\n");
    }else if(I>=11 && I<=13){
        printf("Juvenil A\n");
    }else if(I>=14 && I<=17){
        printf("Juvenil B\n");
    }else if(I>=18){
        printf("Adulto\n");
    }

    return 0;
}