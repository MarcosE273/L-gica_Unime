#include <stdio.h>
#include <math.h>

int main()
{
    int N;
    
    scanf("%d",&N);
    if(N%2==0){
        printf("Numero par\n");
    }else{
        printf("Numero impar\n");
    }
    if(N>=0){
        printf("Numero positivo\n");
    }else{
        printf("Numero negativo\n");
    }

    return 0;
}